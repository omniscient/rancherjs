{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "ordered-imports": [false],
    "no-console": [false],
    "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module", "check-separator", "check-type"],
    "interface-name": [true, "never-prefix"],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "max-line-length": [true, 160],
    "trailing-comma": [false],
    "arrow-parens": false,
    "only-arrow-functions": [false],
    "array-type": [false],
    "prefer-for-of": false,
    "radix": false,
    "space-before-function-paren": [true, {"anonymous": "always", "named": "never", "asyncArrow": "always", "constructor": "never", "method": "never"}]
  }
}
